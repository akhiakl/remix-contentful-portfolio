{
  "name": "contentful-portfolio",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "predev": "pnpm cf-typegen:dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "cf-typegen:dev": "dotenv -e .env -- pnpm cf-typegen",
    "cf-typegen": "cf-content-types-generator -e $CONTENTFUL_ENVIRONMENT -t $CONTENTFUL_MANAGEMENT_ACCESS_TOKEN -s $CONTENTFUL_SPACE_ID -g -d -X --out ./app/contentful/types"
  },
  "dependencies": {
    "@contentful/live-preview": "^4.6.11",
    "@react-router/fs-routes": "^7.4.0",
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "@vercel/react-router": "^1.1.0",
    "cf-content-types-generator": "^2.15.5",
    "clsx": "^2.1.1",
    "contentful": "^11.5.4",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-backend": "^3.0.2",
    "isbot": "^5.1.17",
    "motion": "^12.4.9",
    "nodemailer": "^6.10.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-router": "^7.2.0",
    "remix-i18next": "^7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@react-router/dev": "^7.2.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.25.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}